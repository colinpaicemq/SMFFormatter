  INDD(DUMPINB,OPTIONS(DUMP))
  OUTDD(DUMPOUT,TYPE(42))
  DATE(2020315,2022288)
  START(0000)
  START(2359)
//*
  OUTDD(DUMPOUT,TYPE(120,123,30))
  OUTDD(DUMPOUT,TYPE(30))
  OUTDD(DUMPOUT,TYPE(120,123,30))
//*
