//*MFDUMP  EXEC PGM=IEFBR14
//SMFDUMP  EXEC PGM=IFASMFDP
//DUMPINA  DD   DSN=&SMFPDS,DISP=SHR,AMP=('BUFSP=65536')
//DUMPINB  DD   DSN=&SMFSDS,DISP=SHR,AMP=('BUFSP=65536')
//DUMPOUT  DD   DISP=(NEW,PASS),DSN=&TEMP,SPACE=(CYL,(1,1))
//* MPOUT  DD   DISP=(MOD,CATLG),DSN=COLIN.SMF.OUT,SPACE=(CYL,(10,10))
//SYSPRINT DD   SYSOUT=*
//*YSPRINT DD   DSN=COLIN.SYSPRINT,DISP=(MOD,CATLG),SPACE=(CYL,(1,1))
//SYSIN  DD *
